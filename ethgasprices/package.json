{
  "name": "ethereum-gas-prices",
  "description": "Web3API for Ethereum gas prices.",
  "private": true,
  "version": "0.0.1-prealpha.69",
  "scripts": {
    "build": "yarn build:web3api",
    "build:web3api": "npx w3 build",
    "test:env:up": "npx w3 test-env up",
    "test:env:down": "npx w3 test-env down",
    "deploy": "yarn deploy:web3api",
    "deploy:web3api": "npx w3 build --ipfs http://localhost:5001 --test-ens ethgasprices.eth",
    "test": "npx w3 query ./recipes/e2e.json"
  },
  "devDependencies": {
    "@web3api/cli": "0.0.1-prealpha.69",
    "@web3api/ethereum-plugin-js": "0.0.1-prealpha.69",
    "@web3api/wasm-as": "0.0.1-prealpha.69",
    "assemblyscript": "0.19.1",
    "node-ts-cache": "4.4.0",
    "node-ts-cache-storage-memory" : "4.4.0",
    "csv-parser" : "3.0.0"
  }
}
