{
  "name": "@polywrap/ens-wrapper",
  "description": "ENS Wrapper",
  "private": true,
  "version": "0.0.1-prealpha.84",
  "scripts": {
    "build": "npx polywrap build",
    "test:env:up": "npx polywrap test-env up",
    "test:env:down": "npx polywrap test-env down",
    "deploy": "yarn deploy:contract && yarn deploy:wrapper",
    "deploy:wrapper": "npx polywrap build --ipfs http://localhost:5001 --test-ens ens.eth",
    "deploy:contract": "node ./scripts/deploy-contracts.js",
    "test": "yarn test:e2e && yarn test:env:up && yarn deploy && yarn test:recipe && yarn test:env:down",
    "test:e2e": "jest --passWithNoTests --runInBand --verbose",
    "test:recipe": "npx polywrap query ./recipes/e2e.json"
  },
  "dependencies": {
    "@polywrap/wasm-as": "0.0.1-prealpha.85",
    "assemblyscript": "0.19.5"
  },
  "devDependencies": {
    "@types/jest": "22.2.3",
    "polywrap": "0.0.1-prealpha.85",
    "@polywrap/ethereum-plugin-js": "0.0.1-prealpha.85",
    "@polywrap/test-env-js": "0.0.1-prealpha.85",
    "dotenv": "8.2.0",
    "ethers": "5.0.8",
    "jest": "27.0.6",
    "js-yaml": "3.14.0",
    "typescript": "4.0.7",
    "ts-jest": "27.0.4",
    "ts-node": "9.1.1"
  }
}
