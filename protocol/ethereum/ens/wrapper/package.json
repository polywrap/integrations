{
  "name": "@web3api/ens-api",
  "description": "ENS Web3API",
  "private": true,
  "version": "0.0.1-prealpha.84",
  "scripts": {
    "build": "npx w3 build",
    "test:env:up": "npx w3 infra up --modules=eth-ens-ipfs",
    "test:env:down": "npx w3 infra down --modules=eth-ens-ipfs",
    "deploy": "npx w3 deploy",
    "test": "yarn test:e2e && yarn test:env:up && sleep 10 && yarn deploy && yarn test:recipe && yarn test:env:down",
    "test:e2e": "jest --passWithNoTests --runInBand --verbose",
    "test:recipe": "npx w3 query ./recipes/e2e.json"
  },
  "dependencies": {
    "@web3api/client-js": "^0.0.1-prealpha.84",
    "@web3api/wasm-as": "0.0.1-prealpha.84"
  },
  "devDependencies": {
    "@types/jest": "22.2.3",
    "@web3api/cli": "0.0.1-prealpha.84",
    "@web3api/ethereum-plugin-js": "0.0.1-prealpha.84",
    "@web3api/test-env-js": "0.0.1-prealpha.84",
    "dotenv": "8.2.0",
    "ethers": "5.0.8",
    "jest": "27.0.6",
    "js-yaml": "3.14.0",
    "ts-jest": "27.0.4",
    "ts-node": "9.1.1",
    "typescript": "4.0.7"
  }
}
