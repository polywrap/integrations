{
  "name": "@polywrap/ens-api",
  "description": "ENS polywrap",
  "private": true,
  "version": "0.0.1-prealpha.91",
  "scripts": {
    "build": "npx polywrap build",
    "test:env:up": "npx polywrap infra up --modules=eth-ens-ipfs",
    "test:env:down": "npx polywrap infra down --modules=eth-ens-ipfs",
    "deploy": "npx polywrap deploy",
    "test": "yarn test:e2e && yarn test:env:up && sleep 10 && yarn deploy && yarn test:recipe && yarn test:env:down",
    "test:e2e": "jest --passWithNoTests --runInBand --verbose"
  },
  "dependencies": {
    "@polywrap/client-js": "0.0.1-prealpha.91",
    "@polywrap/wasm-as": "0.0.1-prealpha.91"
  },
  "devDependencies": {
    "@types/jest": "22.2.3",
    "polywrap": "0.0.1-prealpha.91",
    "@polywrap/ens-plugin-js": "0.0.1-prealpha.89",
    "@polywrap/ethereum-plugin-js": "0.0.1-prealpha.91",
    "@polywrap/test-env-js": "0.0.1-prealpha.91",
    "dotenv": "8.2.0",
    "ethers": "5.0.8",
    "jest": "27.0.6",
    "js-yaml": "3.14.0",
    "ts-jest": "27.0.4",
    "ts-node": "9.1.1",
    "typescript": "4.0.7"
  }
}
