    "axios": "0.21.1",
    "ethers": "5.6.4",
    "jest": "26.6.3",
    "js-yaml": "3.14.0",
    "node-fetch": "2.6.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.4",
    "ts-node": "8.10.2",
    "typescript": "4.0.7",
    "@typescript-eslint/eslint-plugin": "4.11.1",
    "@typescript-eslint/parser": "4.11.1",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
