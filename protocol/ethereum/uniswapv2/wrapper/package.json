{
  "name": "@web3api/uniswapV2-api",
  "description": "Uniswap V2 Web3API",
  "private": true,
  "version": "0.0.1-prealpha.84",
  "scripts": {
    "reset": "yarn clean && yarn && yarn build",
    "clean": "npx rimraf ./node_modules ./build ./coverage ./.w3 ./**/w3",
    "build": "npx w3 build",
    "deploy": "npx w3 deploy",
    "test:e2e": "docker-compose up -d && jest --passWithNoTests --runInBand --verbose && docker-compose down -d",
    "test:unit": "asp --verbose",
    "test": "yarn test:unit && yarn test:e2e",
    "test:ci": "yarn test:unit && yarn test:e2e"
  },
  "dependencies": {
    "as-bigfloat": "0.0.4"
  },
  "devDependencies": {
    "@as-pect/cli": "6.2.4",
    "@ethersproject/providers": "5.6.4",
