{
  "name": "templates-wrapper-interface",
  "description": "Polywrap Interface Example",
  "private": true,
  "version": "0.5.0",
  "scripts": {
    "build": "npx polywrap build",
    "test:env:up": "npx polywrap infra up --modules=eth-ens-ipfs",
